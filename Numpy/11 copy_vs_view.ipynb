      "Var : [1 2 8 4]\n",
      "View : [1 2 8 4]\n"
     ]
    }
   ],
   "source": [
    "var = np.array([1,2,3,4])\n",
    "view = var.view()\n",
    "var[2] = 8\n",
    "print('Var :', var)\n",
    "print('View :', view)"
   ]
