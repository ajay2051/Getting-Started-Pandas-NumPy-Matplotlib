     "text": [
      "Var : [1 9 3 4]\n",
      "copy : [1 2 3 4]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "var = np.array([1,2,3,4])\n",
    "co = var.copy()\n",
    "var[1] = 9\n",
    "print('Var :', var)\n",
    "print(\"copy :\", co)"
   ]
  },
